# VoiceFit Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenAI API (for embeddings)
OPENAI_API_KEY=sk-proj-your-openai-api-key-here

# Supabase Configuration
SUPABASE_URL=https://szragdskusayriycfhrs.supabase.co
SUPABASE_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_KEY=your-supabase-service-role-key-here
SUPABASE_JWT_SECRET=your-supabase-jwt-secret-here
SUPABASE_PROJECT_ID=szragdskusayriycfhrs
SUPABASE_REGION=us-east-1

# Database Connection
# Format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://postgres:your_password@db.szragdskusayriycfhrs.supabase.co:5432/postgres

# Nebius API (Fine-tuned Llama 3.3 70B for AI Coach & Voice Parsing)
NEBIUS_API_KEY=your-nebius-api-key-here
NEBIUS_BASE_URL=https://api.tokenfactory.nebius.com/v1
VOICE_MODEL_ID=meta-llama/Llama-3.3-70B-Instruct-fast-LoRa:voice-fit-original-UrGX

# Upstash Search (Exercise Cache - Hybrid Full-Text + Semantic)
UPSTASH_SEARCH_REST_URL=https://your-upstash-search-url.upstash.io
UPSTASH_SEARCH_REST_TOKEN=your-upstash-search-token-here

# xAI API (Grok 4 Fast for Program Generation)
XAI_API_KEY=xai-your-xai-api-key-here

# Authentication (set to true for production/TestFlight)
REQUIRE_AUTH=true

# Weather API (for running features)
WEATHER_API_KEY=your-openweathermap-api-key-here
WEATHER_API_PROVIDER=openweathermap

# Embedding Configuration
EMBEDDING_MODEL=Xenova/all-MiniLM-L6-v2
EMBEDDING_BATCH_SIZE=50
EMBEDDING_COMMIT_INTERVAL=100

# Migration Configuration
MIGRATION_BATCH_SIZE=100
DRY_RUN=false

# Logging
LOG_LEVEL=info
LOG_FILE=migration.log

# Performance
MAX_CONNECTIONS=5
TIMEOUT_MS=30000

