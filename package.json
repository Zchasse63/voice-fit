{
  "name": "voice-fit-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Voice Fit - Voice-first fitness tracking app monorepo",
  "scripts": {
    "mobile": "cd apps/mobile && npm start",
    "mobile:ios": "cd apps/mobile && npx expo run:ios",
    "mobile:build": "cd apps/mobile && eas build",
    "backend": "cd apps/backend && python main.py",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "clean": "rm -rf apps/*/node_modules packages/*/node_modules node_modules"
  },
  "keywords": [
    "voice-fit",
    "fitness",
    "react-native",
    "expo"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "expo-analytics-amplitude": "^11.2.0"
  }
}
