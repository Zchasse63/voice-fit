{
  "name": "voice-fit-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch='**/__tests__/unit/**/*.test.ts' --testMatch='**/__tests__/unit/**/*.test.tsx'",
    "test:e2e:web": "npx playwright test",
    "test:e2e:web:ui": "npx playwright test --ui",
    "test:e2e:web:headed": "npx playwright test --headed",
    "test:e2e:web:report": "npx playwright show-report",
    "test:e2e:ios:build": "detox build --configuration ios.sim.debug",
    "test:e2e:ios:test": "detox test --configuration ios.sim.debug",
    "test:e2e:ios": "npm run test:e2e:ios:build && npm run test:e2e:ios:test",
    "postinstall": "echo '\n⚠️  IMPORTANT: If you ran npx expo prebuild --clean, you must re-add simdjson to ios/Podfile!\n   See SETUP_NOTES.md for details.\n'"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@nozbe/watermelondb": "^0.27.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.1.1",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^7.6.2",
    "@supabase/supabase-js": "^2.79.0",
    "expo": "~53.0.0",
    "expo-haptics": "~14.1.4",
    "expo-location": "^19.0.7",
    "expo-status-bar": "~2.2.3",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.552.0",
    "nativewind": "^4.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-audio-recorder-player": "^4.5.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-maps": "^1.26.18",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.18",
    "victory-native": "^41.20.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@nozbe/with-observables": "^1.6.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "detox": "^20.45.1",
    "detox-expo-helpers": "^0.6.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "patch-package": "^8.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  }
}
